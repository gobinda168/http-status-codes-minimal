{
  "name": "http-status-codes-minimal",
  "version": "1.1.10",
  "description": "Constants that represent various HTTP status codes, inspired by the Java Apache HttpStatus API.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:js": "esbuild src/index.ts --bundle --minify --outfile=dist/index.js --platform=node --target=node14 --sourcemap --format=cjs && esbuild src/index.ts --bundle --minify --outfile=dist/index.mjs --platform=node --target=node14 --sourcemap --format=esm",
    "build": "npm run build:types && npm run build:js",
    "prepare": "npm run build",
    "publish": "npm publish",
    "postpublish": "echo 'Package published successfully!'"
  },
  "keywords": [],
  "author": "Gobinda Das",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.20.2",
    "typescript": "^5.4.2"
  }
}
